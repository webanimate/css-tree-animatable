{
  "name": "css-tree-animatable",
  "version": "1.0.5",
  "description": "Fork of css-tree with all but animatable CSS properties removed",
  "main": "./lib/index",
  "author": "Web Animate <60752454+webanimate@users.noreply.github.com>",
  "license": "MIT",
  "private": false,
  "homepage": "https://github.com/webanimate/css-tree-animatable#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webanimate/css-tree-animatable.git"
  },
  "files": [
    "data",
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "rollup -c",
    "c": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "gen": "node generate.js",
    "fix": "yarn gen && yarn fix:eslint && yarn fix:prettier && yarn build",
    "fix:eslint": "eslint --fix \"**/*.*\"",
    "fix:prettier": "prettier --write \"**/*.*\"",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint \"**/*.*\"",
    "lint:prettier": "prettier --check \"**/*.*\"",
    "up": "yarn upgrade-interactive && ncu -i && yarn"
  },
  "dependencies": {
    "mdn-data-animatable": "^1.1.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-json": "^2.1.2",
    "prettier": "^2.0.5",
    "rollup": "^2.23.1",
    "rollup-plugin-terser": "^6.1.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 11,
      "sourceType": "module"
    },
    "plugins": [
      "json"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:json/recommended",
      "prettier"
    ]
  },
  "prettier": {
    "printWidth": 280,
    "semi": false,
    "singleQuote": true
  },
  "keywords": [
    "css",
    "ast",
    "tokenizer",
    "parser",
    "walker",
    "lexer",
    "generator",
    "utils",
    "syntax",
    "validation",
    "animatable",
    "properties"
  ]
}
